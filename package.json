{
  "name": "ring_string",
  "version": "0.4.1",
  "description": "Convert [a-zA-Z0-9] in a string to their Unicode ring counterparts",
  "main": "dist/es6/rs.min.js",
  "scripts": {
    "terser": "terser dist/es6/rs.js > dist/es6/rs.min.js && rm dist/es6/rs.js",
    "typescript": "tsc --build tsconfig.json",
    "clean": "rimraf -f dist/",
    "build": "npm run clean && npm run typescript && npm run terser",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/ring_string.git"
  },
  "keywords": [
    "ring",
    "string",
    "ring-string",
    "ring_string",
    "unicode",
    "testing",
    "internationalization",
    "i18n",
    "localization",
    "l10n"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/ring_string/issues"
  },
  "homepage": "https://github.com/StoneCypher/ring_string#readme",
  "devDependencies": {
    "rimraf": "^3.0.2",
    "terser": "^4.7.0",
    "typescript": "^3.9.3"
  }
}
